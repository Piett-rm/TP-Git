pipeline {
    agent any

    stages {
        stage('Hello World') {
            steps {
                echo 'Hello, World!'
            }
        }
        stage('merge') {
            steps {
                bat "type *.md > manuel.md"
            }
            post {
                success {
                    archiveArtifacts "manuel.md"
                }
            }
        }
    }
}