pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo  'hello le monde'
            }
        }
        stage('Doc') {
            steps {
                sh  'cat *.md > FeatureManual.md'
            }
        }
    }
    post {
            always {
                archiveArtifacts artifacts: './FeatureManual.md', fingerprint: true
            }
        }
}

